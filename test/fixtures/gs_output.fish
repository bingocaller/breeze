function gs_output
    echo (set_color black)"# "(set_color normal)"On branch: "(set_color --bold white)"master"(set_color normal)
    echo (set_color black)"#"
    echo (set_color black)"#"
    echo (set_color normal)"➤ Changes to be committed:"
    echo (set_color black)"#"
    echo (set_color yellow)"#        new file: "(set_color normal)" [1] "(set_color yellow)"added to index.txt"
    echo (set_color yellow)"#        new file: "(set_color normal)" [2] "(set_color yellow)"added to index, deleted in tree.txt"
    echo (set_color yellow)"#        new file: "(set_color normal)" [3] "(set_color yellow)"added to index, modified in tree.txt"
    echo (set_color magenta)"#          copied: "(set_color normal)" [4] "(set_color magenta)"original/copied in index.txt -> copy/copied in index.txt"
    echo (set_color magenta)"#          copied: "(set_color normal)" [5] "(set_color magenta)"original/copied in index.txt, modified in tree.txt -> copy/copied in index.txt, modified in tree.txt"
    echo (set_color red)"#         deleted: "(set_color normal)" [6] "(set_color red)"deleted from index.txt"
    echo (set_color green)"#        modified: "(set_color normal)" [7] "(set_color green)"modified in index.txt"
    echo (set_color green)"#        modified: "(set_color normal)" [8] "(set_color green)"modified in index, deleted in tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)" [9] "(set_color green)"modified in index and tree.txt"
    echo (set_color blue)"#         renamed: "(set_color normal)"[10] "(set_color blue)"to be renamed in index.txt -> renamed in index.txt"
    echo (set_color blue)"#         renamed: "(set_color normal)"[11] "(set_color blue)"to be renamed in index, deleted from tree.txt -> renamed in index, deleted from tree.txt"
    echo (set_color blue)"#         renamed: "(set_color normal)"[12] "(set_color blue)"to be renamed in index, modified in tree.txt -> renamed in index, modified in tree.txt"
    echo (set_color black)"#"
    echo (set_color normal)"➤ Changes not staged for commit:"
    echo (set_color black)"#"
    echo (set_color yellow)"#        new file: "(set_color normal)"[13] "(set_color yellow)"added to working tree.txt"
    echo (set_color magenta)"#          copied: "(set_color normal)"[14] "(set_color magenta)"original/copied in tree.txt -> copy/copied in tree.txt"
    echo (set_color red)"#         deleted: "(set_color normal)"[15] "(set_color red)"deleted from working tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)"[16] "(set_color green)"modified in working tree.txt"
    echo (set_color blue)"#         renamed: "(set_color normal)"[17] "(set_color blue)"to be renamed in tree.txt -> renamed in tree.txt"
    echo (set_color red)"#         deleted: "(set_color normal)"[18] "(set_color red)"added to index, deleted in tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)"[19] "(set_color green)"added to index, modified in tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)"[20] "(set_color green)"copy/copied in index.txt, modified in tree.txt"
    echo (set_color red)"#         deleted: "(set_color normal)"[21] "(set_color red)"modified in index, deleted in tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)"[22] "(set_color green)"modified in index and tree.txt"
    echo (set_color red)"#         deleted: "(set_color normal)"[23] "(set_color red)"renamed in index, deleted from tree.txt"
    echo (set_color green)"#        modified: "(set_color normal)"[24] "(set_color green)"renamed in index, modified in tree.txt"
    echo (set_color black)"#"
    echo (set_color normal)"➤ Untracked files:"
    echo (set_color black)"#"
    echo (set_color cyan)"#       untracked: "(set_color normal)"[25] "(set_color cyan)"untracked.txt"
    echo (set_color black)"#"
    set_color normal
end